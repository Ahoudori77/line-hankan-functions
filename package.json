    "@azure/data-tables": "^13.3.1",
    "@azure/functions": "^4.6.0",
    "@azure/storage-blob": "^12.28.0",
    "@line/bot-sdk": "^7.6.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "azurite": "^3.31.0",
    "typescript": "^5.6.2"
  }
}
